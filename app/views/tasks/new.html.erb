<h1>Tasks#new</h1>
<p>Find me in app/views/tasks/new.html.erb</p>
<%= form_with model: @task, local: true do |task| %>
  <%= task.label :title %>
  <%= task.text_field :title %>
  <%= task.label :details %>
  <%= task.text_area :details %>
  <%= task.submit %>
<% end %>
<%= link_to 'Back', tasks_path %>
