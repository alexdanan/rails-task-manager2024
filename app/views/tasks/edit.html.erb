<h1>Tasks#edit</h1>
<p>Find me in app/views/tasks/edit.html.erb</p>

<%= form_with model: @task do |task| %>
  <%= task.label :title %>
  <%= task.text_field :title, value: @task.title %>
  <%= task.label :details %>
  <%= task.text_area :details, value: @task.details %>
  <%= task.submit %>
<% end %>

<%= link_to 'Back', tasks_path %>
